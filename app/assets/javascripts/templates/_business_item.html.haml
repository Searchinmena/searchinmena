%div Name: {{ businessItem.get('name') }}
.breadcrumbs{ "ng-include": "'_breadcrumbs.html'" }
%div Description: {{ businessItem.get('description') }}
%div
  Fob price
  {{ businessItem.get('fob_price') }}
  {{ businessItem.get('fob_price_currency') }}
  {{ businessItem.get('fob_price_unit') }}
%div Port {{ businessItem.get('port') }}
%div
  Supply ability:
  {{ businessItem.get('supply_ability_capacity') }}
  {{ businessItem.get('supply_ability_unit') }}
  {{ businessItem.get('supply_ability_frequency') }}
%div Packaging details: {{ businessItem.get('packaging_details') }}

Attributes
%ul 
  %li{ "ng-repeat": "attribute in businessItem.get('attributes')" }
    {{ attribute['name'] }} {{ attribute['value'] }}

Photos
%ul 
  %li.item-img-holder{ "ng-repeat": "photo in businessItem.get('photos')" }
    %img.item-img{ "ng-src": "{{ photo['thumb'] }}" }

Payment Terms
%ul 
  %li{ "ng-repeat": "paymentTerm in businessItem.get('payment_terms')" }
    {{ paymentTerm }}
