- if DevisePresenter.login_link?(controller_name)
  = link_to "Log in", new_session_path(resource_name)
  %br/
- if DevisePresenter.registrations_link?(devise_mapping, controller_name)
  = link_to "Sign up", new_registration_path(resource_name)
  %br/
- if DevisePresenter.forgot_your_password_link?(devise_mapping, controller_name)
  = link_to "Forgot your password?", new_password_path(resource_name)
  %br/
- if DevisePresenter.confirmation_link?(controller_name, devise_mapping)
  = link_to "Didn't receive confirmation instructions?",
  new_confirmation_path(resource_name)
  %br/
- if DevisePresenter.unlock_instructions_link?(devise_mapping,
  controller_name, resource_class)
  = link_to "Didn't receive unlock instructions?",
  new_unlock_path(resource_name)
  %br/
- if devise_mapping.omniauthable?
  - resource_class.omniauth_providers.each do |provider|
    = link_to "Sign in with #{provider.to_s.titleize}",
    omniauth_authorize_path(resource_name, provider)
    %br/
